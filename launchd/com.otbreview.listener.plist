        <string>/bin/bash</string>
        <string>-lc</string>
        <string>cd /Users/REPLACE_ME/chess && /usr/bin/env python3 scripts/otb_listener.py --inbox /Users/REPLACE_ME/OTBReview/inbox --base /Users/REPLACE_ME/OTBReview</string>
